{
    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Fiori App via npm start",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["start"],
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Fiori App via ui5 serve",
        "runtimeExecutable": "ui5",
        "runtimeArgs": ["serve"],
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}"
      }
    ]
  }